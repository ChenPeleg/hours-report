{
  "name": "hours-report",
  "version": "1.1.0",
  "description": "hours report based on git repo",
  "main": "./src/index.js",
  "bin": "./src/index.js",
  "files": [
    "src/**/*"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "git",
    "time",
    "spent",
    "tracking",
    "clock",
    "hours"
  ],
  "type": "module",
  "scripts": {
    "start": "node ./src/index.js",
    "start:debug": "node ./src/index.js --debug",
    "start:xlsx": "node ./src/index.js --format=xlsx",
    "test": " node ./test/test_runner.js integration",
    "e2e": " node ./test/e2e/runAppE2e.test.js",
    "format": "prettier --write .",
    "lint": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChenPeleg/hours-report.git"
  },
  "author": "Chen Peleg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChenPeleg/hours-report/issues"
  },
  "homepage": "https://chenpeleg.github.io/hours-report/",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "prettier-plugin-jsdoc": "^0.4.2",
    "typescript": "^4.9.4"
  }
}
